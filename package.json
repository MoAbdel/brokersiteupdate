{
  "name": "mothebroker-clone",
  "version": "1.0.1",
  "private": true,
  "description": "Mo Abdel - Orange County Mortgage Broker Website",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap && node scripts/submit-indexing-after-build.mjs",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "sitemap": "node scripts/build-sitemap.mjs",
    "indexnow:submit": "node scripts/submit-indexnow.mjs",
    "bing:submit": "node scripts/submit-bing-gsc.mjs",
    "gsc:sitemap": "node scripts/submit-gsc-sitemap.mjs",
    "gsc:indexing": "node scripts/submit-gsc-indexing.mjs",
    "gsc:export": "node scripts/export-gsc-performance.mjs",
    "gsc:prune-candidates": "node scripts/build-prune-candidates.mjs",
    "seo:internal-links": "node scripts/internal-link-audit.mjs",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "next lint --fix"
    ],
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "keywords": [
    "mortgage",
    "broker",
    "orange-county",
    "real-estate",
    "loans"
  ],
  "author": "Mo Abdel",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.536.0",
    "next": "^15.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "google-auth-library": "^10.5.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  }
}
